{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ".",
        "includePattern": "\\.js$",
        "excludePattern": "(node_modules/|docs|scratch/|explorations/|css)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
